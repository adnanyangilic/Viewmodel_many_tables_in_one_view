@model View3model.ViewModels.VeggyVM


<div class="container">

    <p>
        <a asp-action="Create">Create New</a>
    </p>



    <table>
        @foreach (var fruit in Model.Fruits)
        {
            <tr>
                <td>@fruit.FruitName</td>

            </tr>

        }
    </table>
    <hr />
    <table>
        @foreach (var vegetable in Model.Vegetables)
        {
            <tr>
                <td>@vegetable.VegetableName</td>

            </tr>

        }
    </table>
    <hr />
    <table>
        @foreach (var legume in Model.Legumes)
        {
            <tr>
                <td>@legume.LegumeName</td>

            </tr>

        }
    </table>
    <hr />
    <table>
        @foreach (var grain in Model.Grains)
        {
            <tr>
                <td>@grain.GrainName</td>

            </tr>

        }
    </table>

</div>